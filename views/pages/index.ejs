<body>
  <div class="container">
    <div class="text-center pt-5">
      <img src="https://s3.amazonaws.com/assets.docsink.com/error_pencil.jpg" alt="errror">
      <h3 class="fw-500 pt-5">We have run into an error.</h3>
      <p class="error-message fw-500 text-center fs-16"> We apologize for the inconvenience, but it looks like it's one
        of these two things </p>
    </div>

    <div class="message-main mb-5 mt-5">
      <% if (typeof data=='object' && data) { %>
      <div class="col">
      <div class="mt-5">
        <!-- <% if (typeof data=='object' && data) { %> -->
          <table>
            <tr>
              <th>Type</th>
              <th>ID</th>

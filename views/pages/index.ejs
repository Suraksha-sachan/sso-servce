            </tr>
            <tr>
              <td>Department ID</td>
              <td>
                <%= data.departmentID %>
              </td>
            </tr>
            <tr>
              <td>Username</td>
              <td>
                <%= data.username %>
              </td>
            </tr>
            <tr>
              <td>Email</td>
              <td>
                <%= data.email %>
              </td>
            </tr>
            <tr>
              <td>Patient ID</td>
              <td>
                <%= data.patientID %>
              </td>
            </tr>

          </table>
          <!-- <% } %> -->
      </div>
      </div>
      <% } %>
      <div class="col">
      <div class="mt-5 row">
        <div class="col">
          <div class="message">
            <div class="notranslate">
              <ul>
                <p class="error-message fw-500 text-center fs-16">
                  <% if (typeof reason !='undefined' && reason) { %>
                    <%= reason %>
                      <% }else{ %> Internal server error processing SSO request <% } %>
                </p>
              </ul>
            </div>
          </div>
        </div>
      </div>
     </div>
    </div>
